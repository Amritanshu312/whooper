    {"offset": {"line": 840, "column": 0}, "map": {"version":3,"sources":["/turbopack/[project]/containers/posts/Posts.jsx"],"sourcesContent":["\"use client\"\r\nimport Detail from \"@/components/ui/detail/Detail\"\r\nimport styles from \"./posts.module.css\"\r\nimport Post from \"@/components/ui/post/Post\"\r\nimport CreatePost from \"@/components/ui/createPost/CreatePost\"\r\nimport { useUserContext } from \"@/context/getUserInfo\"\r\nimport { GetPosts } from \"@/utils/getPosts\"\r\n\r\nconst Posts = () => {\r\n  const { isAuthenticated, userInfo } = useUserContext();\r\n  const { posts } = GetPosts()\r\n\r\n  return (\r\n    <div className={styles.container}>\r\n      {isAuthenticated ? <Detail info={{ isAuthenticated, userInfo }} /> : null}\r\n      <p className=\"highlight\">{posts.length} POSTS</p>\r\n\r\n      <div className={styles.posts}>\r\n        {posts.map((post) => (\r\n          <Post key={post.id} post={post} />\r\n        ))}\r\n      </div>\r\n\r\n      <CreatePost info={{ isAuthenticated, userInfo }} />\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Posts"],"names":[],"mappings":";;;;;;;;;;;;;AAAA;;;;;;;AAQA,MAAM,QAAQ;;IACZ,MAAM,EAAE,eAAe,EAAE,QAAQ,EAAE,GAAG;IACtC,MAAM,EAAE,KAAK,EAAE,GAAG;IAElB,qBACE,wLAAC;QAAI,WAAW,uJAAO,SAAS;;YAC7B,gCAAkB;gBAAQ,MAAM;oBAAE;oBAAiB;gBAAS;;;;;uBAAQ;0BACrE,wLAAC;gBAAE,WAAU;;oBAAa,MAAM,MAAM;oBAAC;;;;;;;0BAEvC,wLAAC;gBAAI,WAAW,uJAAO,KAAK;0BACzB,MAAM,GAAG,CAAC,CAAC,qBACV;wBAAoB,MAAM;uBAAf,KAAK,EAAE;;;;;;;;;;0BAItB;gBAAY,MAAM;oBAAE;oBAAiB;gBAAS;;;;;;;;;;;;AAGpD;GAlBM;;;;;KAAA;uCAoBS"}},
