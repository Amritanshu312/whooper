    {"offset": {"line": 368, "column": 0}, "map": {"version":3,"sources":["/turbopack/[project]/components/ui/post/Post.jsx"],"sourcesContent":["import Image from \"next/image\"\r\nimport { useState } from \"react\";\r\nimport styles from \"./post.module.css\"\r\nimport { AiOutlineLike, AiFillLike } from \"react-icons/ai\";\r\nimport { BiCommentDetail } from \"react-icons/bi\";\r\n\r\nimport { compareTime } from \"@/utils/Time\";\r\nimport SocialOption from \"./socialOption/SocialOption\";\r\nimport { LikePost } from \"@/utils/handlePost\";\r\n\r\nconst Post = ({ post }) => {\r\n  const { id, uid, likes, likedBy, comments, description, photo, createdAt, user } = post\r\n  const [liked, setLiked] = useState(false)\r\n\r\n  const handleLikeClick = async () => {\r\n    try {\r\n      const isLiked = await LikePost(id, uid)\r\n    } catch (error) {\r\n      console.error('Error liking post:', error.message);\r\n    }\r\n  };\r\n\r\n\r\n  return (\r\n    <div className={styles.container}>\r\n      <div className={styles.top}>\r\n\r\n        <div className={styles.user}>\r\n          <div className={styles.userimage}>\r\n            <Image src={user?.photo} alt=\"profile\" width={50} height={50} />\r\n          </div>\r\n\r\n          <span className={styles.name}>{user?.name}</span>\r\n        </div>\r\n\r\n        <div className={styles.time}>{compareTime(createdAt?.seconds)}</div>\r\n\r\n      </div>\r\n\r\n      <div>\r\n        <div className={styles.description}>\r\n          {description}\r\n        </div>\r\n\r\n        {photo === '' ? null :\r\n          <div className={styles.image}>\r\n            <Image src={photo} alt=\"post\" layout=\"responsive\" width={5000} height={5000} />\r\n          </div>}\r\n      </div>\r\n\r\n      <div className={styles.social}>\r\n        <div className={styles.socialuserinput}>\r\n          <div\r\n            onClick={() => handleLikeClick()}\r\n\r\n            className={likedBy ? likedBy.includes(uid) ? styles.active : null : null}\r\n          >\r\n            <AiOutlineLike /> {likes}\r\n          </div>\r\n\r\n          <div className={styles.comment}><BiCommentDetail /> {comments.length}</div>\r\n        </div>\r\n\r\n        <SocialOption info={{ id, uid }} />\r\n\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Post"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;AAUA,MAAM,OAAO,CAAC,EAAE,IAAI,EAAE;;IACpB,MAAM,EAAE,EAAE,EAAE,GAAG,EAAE,KAAK,EAAE,OAAO,EAAE,QAAQ,EAAE,WAAW,EAAE,KAAK,EAAE,SAAS,EAAE,IAAI,EAAE,GAAG;IACnF,MAAM,CAAC,OAAO,SAAS,GAAG,0KAAS;IAEnC,MAAM,kBAAkB;QACtB,IAAI;YACF,MAAM,UAAU,MAAM,mIAAS,IAAI;QACrC,EAAE,OAAO,OAAO;YACd,QAAQ,KAAK,CAAC,sBAAsB,MAAM,OAAO;QACnD;IACF;IAGA,qBACE,wLAAC;QAAI,WAAW,2JAAO,SAAS;;0BAC9B,wLAAC;gBAAI,WAAW,2JAAO,GAAG;;kCAExB,wLAAC;wBAAI,WAAW,2JAAO,IAAI;;0CACzB,wLAAC;gCAAI,WAAW,2JAAO,SAAS;0CAC9B,cAAA;oCAAO,KAAK,MAAM;oCAAO,KAAI;oCAAU,OAAO;oCAAI,QAAQ;;;;;;;;;;;0CAG5D,wLAAC;gCAAK,WAAW,2JAAO,IAAI;0CAAG,MAAM;;;;;;;;;;;;kCAGvC,wLAAC;wBAAI,WAAW,2JAAO,IAAI;kCAAG,gIAAY,WAAW;;;;;;;;;;;;0BAIvD,wLAAC;;kCACC,wLAAC;wBAAI,WAAW,2JAAO,WAAW;kCAC/B;;;;;;oBAGF,UAAU,KAAK,qBACd,wLAAC;wBAAI,WAAW,2JAAO,KAAK;kCAC1B,cAAA;4BAAO,KAAK;4BAAO,KAAI;4BAAO,QAAO;4BAAa,OAAO;4BAAM,QAAQ;;;;;;;;;;;;;;;;;0BAI7E,wLAAC;gBAAI,WAAW,2JAAO,MAAM;;kCAC3B,wLAAC;wBAAI,WAAW,2JAAO,eAAe;;0CACpC,wLAAC;gCACC,SAAS,IAAM;gCAEf,WAAW,UAAU,QAAQ,QAAQ,CAAC,OAAO,2JAAO,MAAM,GAAG,OAAO;;kDAEpE;;;;;oCAAiB;oCAAE;;;;;;;0CAGrB,wLAAC;gCAAI,WAAW,2JAAO,OAAO;;kDAAE;;;;;oCAAmB;oCAAE,SAAS,MAAM;;;;;;;;;;;;;kCAGtE;wBAAc,MAAM;4BAAE;4BAAI;wBAAI;;;;;;;;;;;;;;;;;;AAKtC;GA1DM;KAAA;uCA4DS"}},
