    {"offset": {"line": 674, "column": 0}, "map": {"version":3,"sources":["/turbopack/[project]/components/ui/createPost/CreatePost.jsx"],"sourcesContent":["\"use client\"\r\nimport { addPost } from \"@/utils/getPosts\";\r\nimport styles from \"./createPost.module.css\"\r\nimport { useState } from \"react\"\r\nimport { FaPlus } from \"react-icons/fa6\";\r\nimport { motion } from \"framer-motion\"\r\n\r\nconst CreatePost = ({ info }) => {\r\n  const { isAuthenticated, userInfo } = info\r\n\r\n  const [isVisible, setIsVisible] = useState(false)\r\n  const [description, setDescription] = useState(\"\")\r\n\r\n  const handleClick = (e) => {\r\n    if (e.target.className.toLowerCase().includes(\"bgcontainer\")) {\r\n      setIsVisible(prev => !prev)\r\n    }\r\n  }\r\n\r\n  const dropIn = {\r\n    hidden: {\r\n      y: \"-100vh\",\r\n      opacity: 0,\r\n    },\r\n    visible: {\r\n      y: \"0\",\r\n      opacity: 1,\r\n      transition: {\r\n        duration: 0.3, // Increased duration for smoother animation\r\n        type: \"spring\",\r\n        damping: 25,\r\n        stiffness: 500,\r\n      },\r\n    }\r\n  };\r\n\r\n  const handlePost = () => {\r\n    try {\r\n      const { uid } = userInfo;\r\n      if (!uid || !description) {\r\n        throw new Error('Missing user ID or description');\r\n      }\r\n\r\n      const data = { uid, description };\r\n      addPost(data, isAuthenticated);\r\n      setIsVisible(false)\r\n      setDescription(\"\")\r\n    } catch (error) {\r\n    }\r\n  };\r\n\r\n\r\n  return (\r\n    <>\r\n      <button className={styles.createicon} onClick={() => setIsVisible(true)}><FaPlus /></button>\r\n\r\n      {isVisible ? <motion.div\r\n        className={styles.bgcontainer}\r\n        onClick={e => handleClick(e)}>\r\n        <motion.div\r\n          className={styles.container}\r\n          variants={dropIn}\r\n          initial=\"hidden\"\r\n          animate={isVisible ? \"visible\" : \"hidden\"}\r\n        >\r\n          <div className={styles.input}>\r\n            <textarea\r\n              cols=\"80\"\r\n              rows=\"15\"\r\n              placeholder={`What's on your mind ? ${userInfo.displayName ? userInfo.displayName : ''}`}\r\n              value={description}\r\n              onChange={(e) => setDescription(e.target.value)}\r\n            ></textarea>\r\n\r\n            <div className={styles.option}>\r\n              <button>Photo</button>\r\n              <button>Gif</button>\r\n              <button>Live</button>\r\n            </div>\r\n          </div>\r\n\r\n\r\n          <button\r\n            className={styles.post}\r\n            onClick={handlePost}\r\n          >Post</button>\r\n        </motion.div>\r\n      </motion.div > : null\r\n      }\r\n    </>\r\n  )\r\n}\r\n\r\nexport default CreatePost"],"names":[],"mappings":";;;;;;;;;;;;AAAA;;;;;;AAOA,MAAM,aAAa,CAAC,EAAE,IAAI,EAAE;;IAC1B,MAAM,EAAE,eAAe,EAAE,QAAQ,EAAE,GAAG;IAEtC,MAAM,CAAC,WAAW,aAAa,GAAG,0KAAS;IAC3C,MAAM,CAAC,aAAa,eAAe,GAAG,0KAAS;IAE/C,MAAM,cAAc,CAAC;QACnB,IAAI,EAAE,MAAM,CAAC,SAAS,CAAC,WAAW,GAAG,QAAQ,CAAC,gBAAgB;YAC5D,aAAa,CAAA,OAAQ,CAAC;QACxB;IACF;IAEA,MAAM,SAAS;QACb,QAAQ;YACN,GAAG;YACH,SAAS;QACX;QACA,SAAS;YACP,GAAG;YACH,SAAS;YACT,YAAY;gBACV,UAAU;gBACV,MAAM;gBACN,SAAS;gBACT,WAAW;YACb;QACF;IACF;IAEA,MAAM,aAAa;QACjB,IAAI;YACF,MAAM,EAAE,GAAG,EAAE,GAAG;YAChB,IAAI,CAAC,OAAO,CAAC,aAAa;gBACxB,MAAM,IAAI,MAAM;YAClB;YAEA,MAAM,OAAO;gBAAE;gBAAK;YAAY;YAChC,gIAAQ,MAAM;YACd,aAAa;YACb,eAAe;QACjB,EAAE,OAAO,OAAO,CAChB;IACF;IAGA,qBACE;;0BACE,wLAAC;gBAAO,WAAW,uKAAO,UAAU;gBAAE,SAAS,IAAM,aAAa;0BAAO,cAAA;;;;;;;;;;YAExE,0BAAY,wLAAC,wLAAO,GAAG;gBACtB,WAAW,uKAAO,WAAW;gBAC7B,SAAS,CAAA,IAAK,YAAY;0BAC1B,cAAA,wLAAC,wLAAO,GAAG;oBACT,WAAW,uKAAO,SAAS;oBAC3B,UAAU;oBACV,SAAQ;oBACR,SAAS,YAAY,YAAY;;sCAEjC,wLAAC;4BAAI,WAAW,uKAAO,KAAK;;8CAC1B,wLAAC;oCACC,MAAK;oCACL,MAAK;oCACL,aAAa,CAAC,sBAAsB,EAAE,SAAS,WAAW,GAAG,SAAS,WAAW,GAAG,GAAG,CAAC;oCACxF,OAAO;oCACP,UAAU,CAAC,IAAM,eAAe,EAAE,MAAM,CAAC,KAAK;;;;;;8CAGhD,wLAAC;oCAAI,WAAW,uKAAO,MAAM;;sDAC3B,wLAAC;sD